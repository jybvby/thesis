: #767676;\n                li {\n                overflow: hidden;\n                text-overflow: ellipsis;\n                white-space: nowrap;\n                }\n            }\n            .dropdown-toggle {\n                .padding-r(26px);\n            }\n        }\n        .c-long-dropdown {\n            min-width: 230px;\n        }\n    }\n}\n\n.products-modal {\n    .c-modal {\n        overflow: hidden;\n    }\n    .modal-dialog {\n        width: 996px;\n        height: 614px;\n    }\n    .modal-footer {\n        display: none;\n    }\n    .sub-title {\n        text-align: center;\n        line-height: 14px;\n        font-size: 12px;\n        padding-bottom: 32px;\n        margin-top: -11px;\n        background-color: white;\n        color: #999;\n    }\n    .advert-notice {\n        margin-bottom: 24px;\n        margin-top: -8px;\n        .flexbox();\n        .align-center();\n        & when (@IS_RW) {\n            background: #DBF0FA;\n        }\n        & when (@IS_SH) {\n            background: #EBF4FF;\n        }\n        \n        padding: 20px 20px 20px 24px;\n        min-height: 60px;\n        .icon-Alert-notice_16px {\n            color: #666;\n            font-size: 16px;\n            &.reverse {\n                transform: rotateY(180deg);\n            }\n        }\n        .icon-close-new {\n            margin-right: 16px;\n            cursor: pointer;\n        }\n        .value {\n            flex: 1;\n            margin: 0 24px 0 12px;\n            >p {\n                line-height: 16px;\n                text-align: initial;\n                font > font {\n                    line-height: 16px;\n                    font-size: 14px;\n                }\n            }\n        }\n    }\n    .imnormal {\n        text-align: center;\n        color: #999;\n        font-size: 14px;\n        line-height: 16px;\n        height: 380px;\n        display: flex;\n        flex-direction: column;\n        justify-content: center;\n        align-items: center;\n        h3 {\n            font-size: 18px;\n            line-height: 21px;\n            margin-top: 12px;\n            color: #999;\n        }\n        p {\n            margin-top: 12px;\n            max-width: 311px;\n        }\n        .try-btn {\n            cursor: pointer;\n            width: 94px;\n            height: 28px;\n            margin-top: 24px;\n            line-height: 28px;\n            text-align: center;\n            border: 1px solid #E5E5E5;\n            color: #999;\n        }\n        .img-span {\n            display: block;\n            margin: 0 auto;\n            background-size: 100%;\n            background-repeat: no-repeat;\n        }\n        .empty {\n            height: 72px;\n            width: 72px;\n            & when (@IS_RW) {\n                background-image: url('/she_dist/rw_images/msg_nodata-6749287dc5.png');\n            }\n            & when (@IS_SH) {\n                background-image: url('/she_dist/images/free-trial-empty-2d2ad8f079.png');\n            }\n        }\n        .error {\n            height: 96px;\n            width: 96px;\n            & when (@IS_RW) {\n                background-image: url('/she_dist/rw_images/offline-5767306d62.png');\n            }\n            & when (@IS_SH) {\n                background-image: url('/she_dist/images/offline-fcc59fe2fc.png');\n            }\n        }\n    }\n    .spining {\n        position: relative;\n        height: 30px;\n        width: 30px;\n        margin: 0 auto;\n        animation: rotate 2.4s linear infinite;\n        i {\n            height: 15px;\n            width: 2px;\n            border-bottom: 9px solid #d8d8d8;\n            display: block;\n            position: absolute;\n            top: 14px;\n            left: 14px;\n            transform-origin: 1px 0px;\n            \n            &:nth-child(1) {\n                opacity: 1;\n            }\n            &:nth-child(2) {\n                transform: rotate(45deg);\n                opacity: .9;\n            }\n            &:nth-child(3) {\n                transform: rotate(90deg);\n                opacity: .8;\n            }\n            &:nth-child(4) {\n                transform: rotate(135deg);\n                opacity: .7;\n            }\n            &:nth-child(5) {\n                transform: rotate(180deg);\n                opacity: .6;\n            }\n            &:nth-child(6) {\n                transform: rotate(225deg);\n                opacity: .5;\n            }\n            &:nth-child(7) {\n                transform: rotate(270deg);\n                opacity: .4;\n            }\n            &:nth-child(8) {\n                transform: rotate(315deg);\n                opacity: .3;\n            }\n        }\n    }\n    @keyframes rotate {\n        from {\n            transform: rotate(0deg);\n        }\n        to {\n            transform: rotate(360deg);\n        }\n    }\n}\n.product-list {\n    height: 472px;\n    overflow: auto;\n    .margin-r(-20px);\n    &::-webkit-scrollbar {\n        display: none;\n    }\n    li {\n        cursor: default;\n        .padding-r(20px);\n        margin-bottom: 40px;\n        width: 20%;\n        display: inline-block;\n        vertical-align: top;\n        font-size: 12px;\n        &:hover .img .she-black-btn {\n            display: block;\n        }\n        \n        .img {\n            position: relative;\n            display: flex;\n            justify-content: center;\n            align-items: center;\n            height: 217px;\n            width: 100%;\n            background-position: center;\n            background-repeat: no-repeat;\n            // background-color: lightblue;\n            background-image: url('/she_dist/images/bg-grey-ba96515e9b.png');\n            background-size: 100%;\n            overflow: hidden;\n            img {\n                position: absolute;\n                top: 0;\n                left: 0;\n                height: 100%;\n                width: 100%;\n      